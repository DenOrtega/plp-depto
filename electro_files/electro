<script type="text/javascript">
	var categoryPage = dojo.query(".breadcrumb_links");
	if (categoryPage.length > 0){
		var categoryName = dojo.trim(dojo.attr(categoryPage[0], 'innerHTML'));
		var mainNavButtons = dojo.query(".dijitButtonText");
		var mainNavTitles = [];
		for (var i=0; i < mainNavButtons.length; i++){ mainNavTitles[i] = dojo.attr(mainNavButtons[i],'innerHTML'); }
		
			for (var x = 0; x < mainNavTitles.length; x++){
				var navItem = dojo.attr(mainNavButtons[x].parentNode.parentNode.parentNode, "id");
				if(mainNavTitles[x] == categoryName && navItem != null){
					dojo.addClass(mainNavButtons[x].parentNode, "mainCategoryPage");
					var newItem = navItem.split("_");
					var navSelection = "HeaderMenu_DropDown_" + newItem[newItem.length - 1];
					navSelection = dojo.byId(navSelection);
					dojo.addClass(navSelection, "mainCategoryPage");
				}
			}		
		
	}
</script>
<!-- END BreadCrumbTrailDisplay.jsp -->	
