<script>
	// Load the dojo.fx module
    dojo.require("dojo.fx");
 
    // Don't forget, when using modules, wrap your code in a dojo.ready
    dojo.ready(function(){
        var wipeButton = dojo.byId("category_header"),
            wipeTarget = dojo.byId("category_categories"),
            wipeHeader = dojo.byId("category_title");
 		
        dojo.connect(wipeButton, "onclick", function(evt){

        	if (wipeTarget.className == "wipe no-mobile" || wipeTarget.className == "no-mobile wipe"){
        		dojo.fx.wipeOut({ node: wipeTarget }).play();
        		
            	dojo.removeClass(wipeTarget, "wipe no-mobile");
            	dojo.removeClass(wipeTarget, "no-mobile");
            	dojo.removeClass(wipeTarget, "no-mobile wipe");
            	
            	dojo.removeClass(wipeHeader, "left_sidebar_header");
            	dojo.addClass(wipeHeader, "left_sidebar_header_closed");
        	} else {
            	dojo.fx.wipeIn({ node: wipeTarget }).play();
            	
        		dojo.addClass(wipeTarget, "wipe no-mobile");
        		
        		dojo.removeClass(wipeHeader, "left_sidebar_header_closed");
            	dojo.addClass(wipeHeader, "left_sidebar_header");
            }
        });
        
           
        var breadcrumb = dojo.byId("WC_BreadCrumbTrailDisplay_div_1");
		if(dojo.trim(searchTermValue) == "" && breadcrumb){
			var wipeTarget = dojo.byId("category_categories"), wipeHeader = dojo.byId("category_title");
			var text = breadcrumb.innerHTML;
			if(text && text.indexOf("|") < 0){
		       	if(wipeTarget && wipeHeader){
		       		if (dojo.trim(wipeTarget.className) != "wipe no-mobile" || wipeTarget.className != "no-mobile"){
				       	dojo.fx.wipeIn({ node: wipeTarget }).play();
		        		            	
		        		dojo.addClass(wipeTarget, "wipe no-mobile");
		        		
		        		dojo.removeClass(wipeHeader, "left_sidebar_header_closed");
		            	dojo.addClass(wipeHeader, "left_sidebar_header");
		       		}
		       	}
			}else{
				if(wipeTarget && wipeHeader){
					if (dojo.trim(wipeTarget.className) != "wipe no-mobile" || wipeTarget.className != "no-mobile"){
				       	dojo.fx.wipeIn({ node: wipeTarget }).play();
		        		            	
		        		dojo.addClass(wipeTarget, "wipe no-mobile");
		        		
		        		dojo.removeClass(wipeHeader, "left_sidebar_header_closed");
		            	dojo.addClass(wipeHeader, "left_sidebar_header");
		       		}
				}
			}
		}
		 
		
    });
</script>

<!-- END CategoryFacetListingDisplay.jspf -->
