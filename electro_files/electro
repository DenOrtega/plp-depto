									jQuery('.mfp-close').addClass('mfp-close_loginHeader');
								}}else{
									jQuery('#loginHeader').removeClass('loginHeader').hide();
									jQuery('.mfp-close').removeClass('mfp-close_loginHeader');
							}    
    					});
					});				
