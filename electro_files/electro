<script type="text/javascript">
dojo.addOnLoad(function() { 
	var stock = 'true';
	var chekbox = document.getElementById('includeStockCheckBox');
	if(null != chekbox && undefined != chekbox){
	if('true'== stock){
	chekbox.checked = false; 
	}else{
	chekbox.checked = true; 
	}
	}
	
	}
);	

function includeOutOfStock(){
var stock = 'true';
if('true'== stock){
	window.location.href = 'http://www.paris.cl/tienda/es/paris/search/electro-accesorios-auto';
}else{
	window.location.href = '';
}
							
};

dojo.require("dojo.fx");
    
    function showHideFacetSidebar(countId){
    	if((document.getElementById("normal_header_"+countId)) && 
    				(document.getElementById("normal_categories_"+countId)) &&
    				(document.getElementById("normal_title_"+countId))){
    		
    		var wipeButton = dojo.byId("normal_header_"+countId);
			var wipeTarget = dojo.byId("normal_categories_"+countId);
			var wipeHeader = dojo.byId("normal_title_"+countId);
    	
	    	if (wipeTarget.className == "wipe"){
	        	dojo.fx.wipeOut({ node: wipeTarget }).play();
	            dojo.removeClass(wipeTarget, "wipe");
	            dojo.removeClass(wipeHeader, "left_sidebar_header");
	            dojo.addClass(wipeHeader, "left_sidebar_header_closed");
	        } else {
	        	dojo.fx.wipeIn({ node: wipeTarget }).play();
	        	dojo.addClass(wipeTarget, "wipe");
	        	dojo.removeClass(wipeHeader, "left_sidebar_header_closed");
	            dojo.addClass(wipeHeader, "left_sidebar_header");
	      	}
		}
    }

</script>
